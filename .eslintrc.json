{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": [
            "error",
            {
              "code": 250,
              "tabWidth": 2,
              "ignoreComments": true, //"comments": 80
              "ignoreUrls": true,
              "ignoreStrings": true,
              "ignoreTemplateLiterals": true
            }
          ],
          "require-jsdoc" : 0
    }
}
